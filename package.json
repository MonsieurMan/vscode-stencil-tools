{
  "name": "vscode-stencil-generator",
  "displayName": "Stencil Generator",
  "description": "Generates Stencil components files",
  "version": "0.0.1",
  "publisher": "natemoo-re",
  "author": {
    "name": "Nate Moore",
    "email": "nate@natemoo.re"
  },
  "homepage": "https://github.com/natemoo-re/vscode-stencil-generator",
  "galleryBanner": {
    "color": "#0273D4",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/natemoo-re/vscode-stencil-generator.git"
  },
  "bugs": {
    "url": "https://github.com/natemoo-re/vscode-stencil-generator/issues",
    "email": "dmitriybaikov@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode-stencil",
    "stencil",
    "stencil component",
    "stencil generate component",
    "vscode stencil",
    "typescript",
    "tsx"
  ],
  "activationEvents": [
    "workspaceContains:**/stencil.config.js",
    "onCommand:extension.generateComponentFiles"
  ],
  "main": "./out/src/extension",
  "icon": "assets/images/icon.png",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Stencil Generator configuration",
      "properties": {
        "stencilGenerator.quotes": {
          "type": ["single", "double"],
          "default": "single",
          "description": "TODO"
        },
        "stencilGenerator.generateFolder": {
          "type": "boolean",
          "default": true,
          "description": "TODO"
        },
        "stencilGenerator.componentsDirectory": {
          "type": "string",
          "default": "./src/components",
          "description": "TODO"
        },
        "stencilGenerator.component.create": {
          "type": "boolean",
          "default": true,
          "description": "TODO"
        },
        "stencilGenerator.component.extension": {
          "type": "string",
          "default": "tsx",
          "description": "TODO"
        },
        "stencilGenerator.component.prefix": {
          "type": ["string", false],
          "default": false,
          "description": "TODO"
        },
        "stencilGenerator.component.shadow": {
          "type": "boolean",
          "default": false,
          "description": "TODO"
        },
        "stencilGenerator.test.create": {
          "type": "boolean",
          "default": true,
          "description": "TODO"
        },
        "stencilGenerator.test.extension": {
          "type": "string",
          "default": "spec.tsx",
          "description": "TODO"
        },
        "stencilGenerator.style.create": {
          "type": "boolean",
          "default": true,
          "description": "TODO"
        },
        "stencilGenerator.style.extension": {
          "type": "string",
          "default": "css",
          "description": "TODO"
        }
    }
    },
    "commands": [
      {
        "command": "extension.newProjectComponent",
        "title": "New Project – Component",
        "category": "Stencil Generator"
      },
      {
        "command": "extension.newProjectApp",
        "title": "New Project – App",
        "category": "Stencil Generator"
      },
      {
        "command": "extension.generateComponentFiles",
        "title": "Generate Stencil Component"
      },
      {
        "command": "extension.generateComponentFilesFromCommandPalette",
        "title": "New Component",
        "category": "Stencil Generator"
      }
    ],
    "menus": {
      "commandPalette": [{
        "command": "extension.generateComponentFilesFromCommandPalette",
        "when": "isStencilProject"
      }, {
        "command": "extension.generateComponentFiles",
        "when": "!resourceIsFile"
      }],
      "explorer/context": [{
          "command": "extension.generateComponentFiles",
          "when": "explorerResourceIsFolder && isStencilProject",
          "group": "Stencil@0"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "tsc -watch -p ./",
    "build": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "change-case": "^3.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "^0.30.0",
    "rxjs": "^5.0.0-rc.1"
  }
}
