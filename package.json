{
  "name": "vscode-stencil-generator",
  "displayName": "Stencil Generator",
  "description": "Generates Stencil components files",
  "version": "0.0.1",
  "publisher": "natemoo-re",
  "author": {
    "name": "Nate Moore",
    "email": "nate@natemoo.re"
  },
  "homepage": "https://github.com/natemoo-re/vscode-stencil-generator",
  "galleryBanner": {
    "color": "#0273D4",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/natemoo-re/vscode-stencil-generator.git"
  },
  "bugs": {
    "url": "https://github.com/natemoo-re/vscode-stencil-generator/issues",
    "email": "dmitriybaikov@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode-stencil",
    "stencil",
    "stencil component",
    "stencil generate component",
    "vscode stencil",
    "typescript",
    "tsx"
  ],
  "activationEvents": [
    "onCommand:extension.generateComponentFiles"
  ],
  "main": "./out/src/extension",
  "icon": "assets/images/icon.png",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "stencilGenerator",
      "properties": {
        "stencilGenerator.config": {
          "type": "object",
          "description": "vscode-stencil-generator configuration object. See: https://github.com/natemoo-re/vscode-stencil-generator/blob/master/README.md for more information."
        }
      }
    },
    "commands": [
      {
        "command": "extension.generateComponentFiles",
        "title": "New Stencil Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.generateComponentFiles",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "change-case": "^3.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "^0.30.0",
    "rxjs": "^5.0.0-rc.1",
    "lodash": "^4.17.4"
  }
}
